{
  "name": "shalvah/monolog-pusher",
  "description": "Monolog handler for sending logs to Pusher",
  "keywords": [
    "pusher",
    "monolog",
    "logging",
    "error",
    "logs"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Shalvah A",
      "email": "shalvah.adebayo@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "pusher/pusher-php-server": "^4.0",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Shalvah\\MonologPusher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shalvah\\MonologPusher\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  },
  "scripts": {
    "test": "phpunit tests/"
  }
}
